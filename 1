# python file for VMSTAT 
import csv
from subprocess import check_output

vm_stat = []
def read_vmstat_output():
    with open('test.csv', 'wb') as file_excel:
        for x in check_output(['df','-h']).strip().splitlines():
            vm_stat.append(x)
        wr = csv.writer(file_excel, quoting=csv.QUOTE_ALL)
        wr.writerowsx(vm_stat)


read_vmstat_output()

